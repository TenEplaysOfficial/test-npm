{
  "name": "test-first-package-npm",
  "private": false,
  "version": "1.0.1",
  "type": "module",
  "files": [
    "dist",
    "README.md"
  ],
  "main": "./dist/test-first-package-npm.umd.cjs",
  "module": "./dist/test-npm.js",
  "exports": {
    ".": {
      "import": "./dist/test-first-package-npm.js",
      "require": "./dist/test-first-package-npm.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "prebuild": "rimraf dist",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepublish": "npm run build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs --no-cache",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "vite-tsconfig-paths": "^5.1.3"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.2",
    "@eslint/js": "^9.15.0",
    "@storybook/addon-essentials": "^8.4.6",
    "@storybook/addon-interactions": "^8.4.6",
    "@storybook/addon-onboarding": "^8.4.6",
    "@storybook/blocks": "^8.4.6",
    "@storybook/builder-vite": "^8.4.6",
    "@storybook/react": "^8.4.6",
    "@storybook/react-vite": "^8.4.6",
    "@storybook/test": "^8.4.6",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.15.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "eslint-plugin-storybook": "^0.11.1",
    "globals": "^15.12.0",
    "postcss": "^8.4.49",
    "rimraf": "^6.0.1",
    "storybook": "^8.4.6",
    "tailwindcss": "^3.4.15",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.15.0",
    "vite": "^6.0.1",
    "vitepress": "^1.5.0"
  },
  "homepage": "https://github.com/teneplaysofficial/test-npm",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teneplaysofficial/test-npm.git"
  },
  "bugs": {
    "url": "https://github.com/teneplaysofficial/test-npm/issues"
  },
  "keywords": [
    "npm",
    "react",
    "vite",
    "library"
  ],
  "author": "Sriman",
  "license": "MIT",
  "description": "A simple test lib",
  "directories": {
    "lib": "lib"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}